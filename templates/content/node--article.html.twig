{#
/**
    Display an Article page.
 */
#}

<article{{ attributes }}>

  <div class="article-content">
    <h1{{ title }}> {{ label }}</h1>
    {{ content }}
  </div>

  <div class="article-autor">
    <h3>{{ author_name }}</h3>
    <h3>{{ date }}</h3>
  </div>

  <div class="article-quote">
    <a class="ukraine" href="/ukraine"><h5>{{ vice_quote }}</h5></a>
  </div>
  
</article>
